{
    "name": "hepplerdotnet/laravel-http2serverpush",
    "description": "A HTTP2 Server Push Middleware for Laravel 5",
    "keywords": [
        "laravel",
        "laravel-http2serverpush",
        "serverpush"
    ],
    "homepage": "https://github.com/HepplerDotNet/laravel-http2serverpush",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Bennett",
            "email": "me@jakebennett.net",
            "homepage": "https://jakebennett.net",
            "role": "Developer"
        },
        {
            "name": "Patrick Heppler",
            "email": "patrick@heppler.net",
            "homepage": "https://www.heppler.net",
            "role": "Developer"
        }
    ],
    "require": {

        "php" : "^7.0|^8.0",
        "illuminate/support": "*",
        "illuminate/http": "*",
        "symfony/dom-crawler": "^2.7|^3.0|^4.0|^5.0",
        "symfony/css-selector": "^2.7|^3.0|^4.0|^5.0",
        "illuminate/support": "~5.3|~6.0|~7.0|~8.0",
        "illuminate/http": "~5.3|~6.0|~7.0|~8.0",
        "symfony/dom-crawler": "^2.7|^3.0|^4.0|^5.0",
        "symfony/css-selector": "^2.7|^3.0|^4.0|^5.0"

    },
    "require-dev": {
        "phpunit/phpunit" : "^4.0|^5.0",
        "scrutinizer/ocular": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "HepplerDotNet\\Http2ServerPush\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HepplerDotNet\\Http2ServerPush\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "HepplerDotNet\\Http2ServerPush\\ServiceProvider"
            ]
        }
    }
}
